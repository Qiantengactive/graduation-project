        $(".select-seller span:nth-child(3) b").css("backgroundImage", "url(/img/index-1-3.png)")
        $(".select-seller span:nth-child(4) b").css("backgroundImage", "url(/img/index-1-4.png)")
        $(".select-seller span:nth-child(5) b").css("backgroundImage", "url(/img/index-1-5.png)")
