<script>
    function deletLength1() {
        var a = $(".home-body-gongying-body-1").length;
        console.log(a);
        if (a > 4) {
            for (var i = 5; i < a + 1; i++) {
                $(".home-body-gongying-body-1:nth-child(" + i + ")").css("display", "none");
            }
        }
    }
    function deletLength2() {
        var a = $(".a-2 a").length;
        console.log(a);
        if (a > 4) {
            for (var i = 5; i < a + 1; i++) {
                $(".a-2 a:nth-child(" + i + ")").css("display", "none");
            }
        }
