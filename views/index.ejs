    var DomobjAll = $(".a-1").html();
    $(".select-seller span:nth-child(1)").click(function () {
        $(".a-1").empty().append(DomobjAll);
        $(".select-seller span:not(.select-seller span:nth-child(1))").css("backgroundImage", "url(/img/bg-index-_05.png)");
