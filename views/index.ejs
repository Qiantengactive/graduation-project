<script>
    $(document).ready(function () {
        var obj = $.cookie("isFirst");
        if (!obj) {
            /*不存在*/
            $.cookie("isFirst", "1");
            $(".guide-welcome").css("display", "block")
            setTimeout(function () {
                $(".guide-welcome").fadeOut(2000);
            }, 2000)
        } else {
            $.cookie("isFirst", "1");
        }
        var mySwiper = new Swiper('.swiper-container', {
            autoplay: 5000,
            loop: true,
            pagination: '.swiper-pagination'
        });

        var mySwiper = new Swiper('.swiper-container-topic', {
            autoplay: 3000,
            loop: true,
            direction: 'vertical'
        });
        /*选择框-默认选择全部*/
        $(".be-select-2-span").css("backgroundImage", "url(/img/bg-index-_03.png)");
        $(".be-select-2-img").css("backgroundImage", "url(/img/index-2-1.png)");
        $(".be-select-2-p").css("color", "#ffffff");
        $(".be-select-2-span-2").css("backgroundImage", "url(/img/bg-index-_03.png)");
        $(".be-select-2-img-2").css("backgroundImage", "url(/img/index-2-1.png)");
        $(".be-select-2-p-2").css("color", "#ffffff");
    })
</script>
